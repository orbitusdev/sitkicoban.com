  "homepage": "https://orbitus.dev",
  "prettier": "@orbitusdev/prettier-config",
  "scripts": {
    "sw": "esbuild src/app/sw.ts --bundle --format=esm --target=es2022 --outfile=public/sw.js && prettier --write public/sw.js",
    "predev": "pnpm run sw",
    "dev": "next dev",
    "prebuild": "pnpm run sw",
    "build": "next build",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "postinstall": "prisma generate --schema=node_modules/@orbitusdev/database/prisma/schema.prisma"
  },
