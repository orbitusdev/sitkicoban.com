    "@orbitusdev/i18n": "latest",
    "@orbitusdev/api": "latest",
    "@prisma/client": "^6.19.0",
    "import-in-the-middle": "^2.0.0",
    "next": "16.0.8",
    "next-auth": "5.0.0-beta.30",
    "next-intl": "^4.5.8",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "require-in-the-middle": "^8.0.1",
